{
    "name": "nati/offsite-backup-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Symfony bundle to sync a path to a S3 repository",
    "authors": [
        {
            "name": "Antoine Rolland",
            "email": "antoine@nati.tech"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "symfony/config": "^5.0",
        "symfony/console": "^5.0",
        "symfony/http-kernel": "^5.2",
        "aws/aws-sdk-php": "dev-feature-upload-once@dev",
        "symfony/dependency-injection": "^5.2",
        "symfony/yaml": "^5.2",
        "symfony/monolog-bundle": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Nati\\OffsiteBackupBundle\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/natitech/aws-sdk-php",
            "no-api": true
        }
    ]
}
